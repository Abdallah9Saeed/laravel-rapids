{
  "name": "alrik11es/laravel-rapids",
  "description": "Rapid prototyping for laravel.",
  "keywords": ["laravel", "scaffolding", "rapids", "prototyping", "crud", "datatable", "form"],
  "license": "MIT",
  "authors": [
    {
      "name": "Marcos Sigueros",
      "email": "msigueros@servidores.red"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "laravelcollective/html": "~5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.2",
    "laravel/laravel": "~5.2"
  },
  "autoload": {
    "psr-4": {
      "Laravel\\Rapids\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
