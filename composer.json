{
  "name": "alrik11es/rapids",
  "description": "Rapid prototyping for laravel.",
  "keywords": ["laravel", "scaffolding", "rapids", "prototyping"],
  "license": "MIT",
  "authors": [
    {
      "name": "Marcos Sigueros",
      "email": ""
    }
  ],
  "require": {
    "php": ">=7.0.0"

  },
  "require-dev": {
    "phpunit/phpunit": "~5.2",
    "laravel/laravel": "~5.2"
  },
  "autoload": {
    "psr-4": {
      "Laravel\\Rapids\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}